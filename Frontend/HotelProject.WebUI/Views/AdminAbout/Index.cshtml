@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

@*<a href="#" class="btn mb-1 btn-rounded btn-outline-primary">Ekle</a>*@

<br />

<table class="table table-hover">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>İşlemler</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>@item.Title1</td>
            <td>@item.Title2</td>
            <td>
                <a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn mb-1 btn-rounded btn-outline-info">Güncelle</a>
            </td>
        </tr>
    }
</table>

